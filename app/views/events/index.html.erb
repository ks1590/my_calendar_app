<section>
  <div class="container-fruid">
    <div class="row justify-content-center">
      <div class="flex col-xl-12 col-lg-12 col-md-12">            
        <div id="calendar"></div>          
      </div>
    </div>
  </div>    
</section>

  <button type="button" class="btn btn-primary col-2 offset-5 mt-5" data-toggle="modal" data-target="#myModal">
    電卓
  </button>
  <%# <div class="modal fade" id="myModal" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true"> %>
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="">
    <div class="modal-dialog modal-dialog-centered">
      <div class="modal-content">
        <div class="close-content">
          <button class="btn-close" data-dismiss="modal">✖︎</button>
        </div>
        <div class="calculator-grid">
          <div class="output">
            <div data-previous-operand class="previous-operand"></div>
            <div data-current-operand class="current-operand"></div>
          </div>

          <button data-all-clear class="span-two">AC</button>
          <button data-delete class="btn-calc">DEL</button>
          <button data-operation class="btn-calc">÷</button>
          <button data-number class="btn-calc">1</button>
          <button data-number class="btn-calc">2</button>
          <button data-number class="btn-calc">3</button>
          <button data-operation class="btn-calc">×</button>
          <button data-number class="btn-calc">4</button>
          <button data-number class="btn-calc">5</button>
          <button data-number class="btn-calc">6</button>
          <button data-operation class="btn-calc">+</button>
          <button data-number class="btn-calc">7</button>
          <button data-number class="btn-calc">8</button>
          <button data-number class="btn-calc">9</button>
          <button data-operation class="btn-calc">-</button>
          <button data-number class="btn-calc">.</button>
          <button data-number class="btn-calc">0</button>
          <button data-equals class="span-two">=</button>
        </div>
      </div>
    </div>
  </div>


